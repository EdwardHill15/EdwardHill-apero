@book{Chalmers1995,
abstract = {What is consciousness? As the physical processes in the brain give rise to the self - conscious mind and feelings so deeply varied as love or hate, aesthetic pleasure or spiritual yearning ? These questions are now among the most debated issues among scientists and philosophers , and we have seen in recent years superb volumes by eminent figures as Francis Crick , Daniel C. Dennett , Gerald Edelman , and Roger Penrose , all firing volleys in what has come to be called the consciousness wars . Now , in the conscious mind , philosopher David J. Chalmers offers a cogent analysis of this debate as it reveals a major new theory of consciousness that rejects the prevailing reductionist trend of science , offering provocative ideas about the relationship between mind and brain . Writing in a rigorous thought-provoking style, the author takes us on a tour through the far-reaching philosophical implications of consciousness. Chalmers convincingly reveals how contemporary cognitive science and neurobiology have failed to explain how and why mental events emerge from physiological occurrences in the brain . He proposes instead that conscious experience must be understood in an entirely new light - as an irreducible entity (similar to such physical properties as time , mass , and space) that exists at a fundamental level , and can not be understood as the sum of its parts . And after suggesting some intriguing possibilities about the structure and laws of conscious experience , he details how his unique reinterpretation of the mind could be the focus of a new science . Throughout the book , Chalmers provides fascinating thought experiments that trenchantly illustrate his ideas . For example , in exploring the notion that consciousness could be experienced by machines as well as humans , Chalmers asks us to imagine a thinking brain in which neurons are slowly replaced by silicon chips that precisely duplicate their functions - how neurons are replaced , will consciousness gradually fade away? The book also features thoughtful discussions of how the author's theories can be practically applied to subjects as diverse as artificial intelligence and interpretation of quantum mechanics . We have all pondered the nature and meaning of consciousness. Engaging and penetrating, the conscious mind adds a new perspective to the subject that is sure to spark debate about our understanding of mind for years to come . O que {\'{e}} a consci{\^{e}}ncia? Como os processos f{\'{i}}sicos no c{\'{e}}rebro dar lugar {\`{a}} mente auto-consciente e sentimentos t{\~{a}}o profundamente variados como amor ou {\'{o}}dio, prazer est{\'{e}}tico ou anseio espiritual? Essas perguntas hoje est{\~{a}}o entre as quest{\~{o}}es mais debatidas entre os cientistas e fil{\'{o}}sofos, e temos visto nos {\'{u}}ltimos anos excelentes volumes por figuras eminentes como Francis Crick, Daniel C. Dennett, Gerald Edelman, e Roger Penrose, todos os voleios de tiro em que tem veio a ser chamado as guerras consci{\^{e}}ncia. Agora, na mente consciente, fil{\'{o}}sofo David J. Chalmers oferece uma an{\'{a}}lise convincente deste debate como ele revela uma nova e importante teoria da consci{\^{e}}ncia, que rejeita a tend{\^{e}}ncia reducionista predominante da ci{\^{e}}ncia, oferecendo id{\'{e}}ias provocativas sobre a rela{\c{c}}{\~{a}}o entre mente e c{\'{e}}rebro. Escrevendo em um estilo instigante rigoroso, o autor nos leva em uma turn{\^{e}} de grande alcance atrav{\'{e}}s das implica{\c{c}}{\~{o}}es filos{\'{o}}ficas da consci{\^{e}}ncia. Chalmers convincente revela como contempor{\^{a}}nea a ci{\^{e}}ncia cognitiva ea neurobiologia n{\~{a}}o conseguiram explicar como e por que os eventos mentais emergem de ocorr{\^{e}}ncias fisiol{\'{o}}gicas no c{\'{e}}rebro. Ele prop{\~{o}}e vez que a experi{\^{e}}ncia consciente deve ser entendida sob uma luz inteiramente nova - como uma entidade irredut{\'{i}}vel (semelhante a propriedades f{\'{i}}sicas tais como o tempo, em massa, e espa{\c{c}}o) que existe a um n{\'{i}}vel fundamental, e n{\~{a}}o pode ser entendida como a soma das suas partes . E depois de sugerir algumas possibilidades intrigantes sobre a estrutura e as leis da experi{\^{e}}ncia consciente, ele detalha como sua reinterpreta{\c{c}}{\~{a}}o {\'{u}}nica da mente poderia ser o foco de uma nova ci{\^{e}}ncia. Ao longo do livro, Chalmers fornece experimentos mentais fascinantes que incisivamente ilustrar suas id{\'{e}}ias. Por exemplo, em explorar a no{\c{c}}{\~{a}}o de que a consci{\^{e}}ncia poderia ser experimentado por m{\'{a}}quinas, assim como os seres humanos, Chalmers nos pede para imaginar um c{\'{e}}rebro a pensar em que os neur{\^{o}}nios est{\~{a}}o lentamente substitu{\'{i}}das por chips de sil{\'{i}}cio que, precisamente duplicar as suas fun{\c{c}}{\~{o}}es - como os neur{\^{o}}nios s{\~{a}}o substitu{\'{i}}dos, vai consci{\^{e}}ncia gradualmente desaparecer? O livro tamb{\'{e}}m apresenta discuss{\~{o}}es pensativo de como as teorias do autor pode ser aplicado praticamente a temas t{\~{a}}o diversos como a intelig{\^{e}}ncia artificial ea interpreta{\c{c}}{\~{a}}o da mec{\^{a}}nica qu{\^{a}}ntica. Todos n{\'{o}}s j{\'{a}} ponderou sobre a natureza eo significado da consci{\^{e}}ncia. Envolvente e penetrante, a mente consciente acrescenta uma nova perspectiva para o assunto que {\'{e}} certo para despertar o debate sobre a nossa compreens{\~{a}}o da mente para os pr{\'{o}}ximos anos.},
address = {Oxford},
author = {Chalmers, David J.},
doi = {10.1136/bmj.2.5310.969-c},
file = {:C\:/Users/Gebruiker/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Chalmers - 1995 - The Conscious Mind. In Search of a Theory of Conscious Experience.pdf:pdf},
issn = {0959-8138},
publisher = {Oxford University Press},
title = {{The Conscious Mind. In Search of a Theory of Conscious Experience}},
year = {1995}
}
@article{Chalmers1995a,
abstract = {To make progress on the problem of consciousness, we have to confront it directly. In this paper, I first isolate the truly hard part of the problem, separating it from more tractable parts and giving an account of why it is so difficult to explain. I critique some recent work that uses reductive methods to address consciousness, and argue that such methods inevitably fail to come to grips with the hardest part of the problem. Once this failure is recognized, the door to further progress is opened. In the second half of the paper, I argue that if we move to a new kind of nonreductive explanation, a naturalistic account of consciousness can be given. I put forward my own candidate for such an account: a nonreductive theory based on principles of structural coherence and organizational invariance, and a double-aspect theory of information.},
author = {Chalmers, David J. and Chalmers, David J.},
doi = {10.1093/acprof:oso/9780195311105.003.0001},
file = {:C\:/Users/Gebruiker/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Chalmers, Chalmers - 1995 - Facing Up to the Problem of Consciousness.pdf:pdf},
journal = {Journal of Consciousness Studies},
number = {3},
pages = {200--219},
title = {{Facing Up to the Problem of Consciousness}},
volume = {2},
year = {1995}
}
